<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<!-- POLICECASE -->
	<bean id="adminPoliceCaseController" class="cn.gov.dl.ga.gxga.core.controller.BaseController">
		<property name="processMap">
			<map>
				<entry key="default">
					<bean class="cn.gov.dl.ga.gxga.process.DefaultProcess">
						<property name="successView" value="admin/policecase/policecase_list" />
					</bean>
				</entry>
				<entry key="search">
					<bean
						class="cn.gov.dl.ga.gxga.process.admin.policecase.PoliceCaseSearchProcess">
						<property name="articleService" ref="articleService" />
						<property name="successView" value="jsonView" />
					</bean>
				</entry>
				<entry key="createpre">
					<bean class="cn.gov.dl.ga.gxga.process.DefaultProcess">
						<property name="successView" value="admin/policecase/policecase_create" />
					</bean>
				</entry>
				<entry key="create">
					<bean
						class="cn.gov.dl.ga.gxga.process.admin.policecase.PoliceCaseCreateProcess">
						<property name="articleService" ref="articleService" />
						<property name="successView" value="jsonView" />
					</bean>
				</entry>
				<entry key="updatedispatcher">
					<bean class="cn.gov.dl.ga.gxga.process.DefaultProcess">
						<property name="successView"
							value="admin/policecase/policecase_updatedispatcher" />
					</bean>
				</entry>
				<entry key="updatepre">
					<bean
						class="cn.gov.dl.ga.gxga.process.admin.policecase.PoliceCaseUpdatePreProcess">
						<property name="articleService" ref="articleService" />
						<property name="successView" value="admin/policecase/policecase_update" />
					</bean>
				</entry>
				<entry key="update">
					<bean
						class="cn.gov.dl.ga.gxga.process.admin.policecase.PoliceCaseUpdateProcess">
						<property name="articleService" ref="articleService" />
						<property name="successView" value="jsonView" />
					</bean>
				</entry>
				<entry key="updatedetailsearch">
					<bean class="cn.gov.dl.ga.gxga.process.DefaultProcess">
						<property name="successView"
							value="admin/policecase/policecase_updatedetailsearch" />
					</bean>
				</entry>
				<entry key="delete">
					<bean
						class="cn.gov.dl.ga.gxga.process.admin.policecase.PoliceCaseDeleteProcess">
						<property name="articleService" ref="articleService" />
						<property name="successView" value="jsonView" />
					</bean>
				</entry>
			</map>
		</property>
	</bean>
</beans>